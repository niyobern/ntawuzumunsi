option_settings:
  aws:elasticbeanstalk:application:environment:
    PYTHONPATH: "/var/app/current:$PYTHONPATH"
  aws:elasticbeanstalk:container:python:
    WSGIPath: "main:app"

container_commands:
  01_initdb:
    command: "source /var/app/venv/*/bin/activate && sudo almbic upgrade head"
    leader_only: true
    cwd: "var/app"